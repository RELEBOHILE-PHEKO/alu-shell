#!/usr/bin/env bash
# Find the process ID (PID) of the "4-to_infinity_and_beyond" process
pid=$(pgrep "4-to_infinity_and_beyond")

# Check if the process ID is not empty
if [ -n "$pid" ]; then
    echo "Stopping 4-to_infinity_and_beyond process..."
    kill "$pid"
    echo "4-to_infinity_and_beyond process has been stopped."
else
    echo "4-to_infinity_and_beyond process is not running."
fi
